                                                <field name="total_2011" invisible="0" string="Year 2011" attrs="{'invisible': [('total_2011', '==', 0)]}"/>
                                                <field name="total_2012" invisible="0" string="Year 2012" attrs="{'invisible': [('total_2012', '==', 0)]}"/>
